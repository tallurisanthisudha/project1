<div class='employeemaincont'>
<%=render "admin/adminmenu" %>
<div class="empsubcon">
<h1>PROJECT MANAGEMENT</h1>
<table class="table">
    <tr>
      <th>Project_Name</th>
      <th>Project_Description</th>
      <th>Project_Edit</th>
    </tr>
<% @projects.each do |project| %>
    <tr>
     <td>
     <%= project.project_name%>
     </td>
     <td>
     <%= project.description%>
     </td>
<% end %>
</table>
<%= button_to 'Add Project', projectform_path,method: :get,class:"submit-button" %>
</div>
</div>