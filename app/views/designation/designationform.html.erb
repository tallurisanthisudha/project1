<div class="d-flex flex-row">
<%=render "admin/adminmenu" %>
<div class="designationsubcon">
<h1>DESIGNATION MANAGEMENT</h1>
<%= form_with  model: @designation do |f| %>
    <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
    </div>
 
    <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description, autofocus: true, autocomplete: "description" %>
    </div>

    <div class="actions ">
        <%= f.submit "Add",class:"loginbutton" %>
    </div>


  <% end %>
</div>
</div>