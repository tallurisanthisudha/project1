<div class='employeemaincont'>
<%=render "admin/adminmenu" %>
<div class="empsubcon">
<h1>Leave Management</h1>
<table class="table">
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
<% @leaves.each do |leave| %>
    <tr>
     <td>
     <%= leave.name%>
     </td>
     <td>
     <%= leave.description%>
     </td>
     <td>
     <%= link_to "Edit", edit_leafe_path(leave),class:"btn btn-primary" %>
     </td>
     <td>
     <%= button_to "Delete", leafe_path(leave),method: :delete,class:"btn btn-secondary" %>
     </td>
    <% end %>
</table>
<%= button_to 'Add Leave', new_leafe_path,method: :get,class:"loginbutton" %>
</div>
</div>