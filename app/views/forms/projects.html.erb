<div>
<%= form_with do @role|form| %>
    <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
    </div>
 
    <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description, autofocus: true, autocomplete: "description" %>
    </div>

    <div class="field">
    <%= f.label :project_lead_id%><br />
    <%= f.number_field :project_lead_id, autofocus: true, autocomplete: "project_lead_id" %>
    </div>

    <div class="field">
    <%= f.label :manager_id %><br />
    <%= f.number_field :manager_id, autofocus: true, autocomplete: "manager_id" %>
    </div>
    </div>
  <% end %>